{
  "name": "dod_miner",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "scripts": {
    "build:swc": "rm -rf dist && swc src --out-dir dist",
    "build": "tsc --project './tsconfig.json'",
    "build:2": "tsc --build",
    "start": "rm -rf dist && pnpm run build:swc && NODE_PORT=3006 NODE_ENV=production node --experimental-specifier-resolution=node  ./dist/main.js",
    "miner": "rm -rf dist && pnpm run build:swc && node ./dist/cli.js",
    "benchmark": "rm -rf dist && pnpm run build:swc && node ./dist/benchmarks.js",
    "watch:2": "tsc --watch",
    "watch": "tsc --project './tsconfig.json' --watch",
    "clean": "rm -rf dist node_modules/.cache tsconfig.tsbuildinfo",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}'",
    "test": "ts-mocha test/**/*.spec.ts --exit --timeout 10000",
    "precommit": "lint-staged",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm run lint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
